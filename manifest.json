{
  "manifest_version": 3,
  "name": "Gemini Extension",
  "version": "1.0",
  "description": "A Chrome extension for Gemini",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "background": {
    "service_worker": "index.js"
  },
  "action": {
    "default_popup": "index.html"
  }
}